{
  "kind": "collectionType",
  "collectionName": "variantes",
  "info": {
    "singularName": "variante",
    "pluralName": "variantes",
    "displayName": "Variante",
    "mainField": "Nombre"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string"
    },
    "Color": {
      "type": "string",
      "required": true
    },
    "Talla": {
      "type": "string",
      "required": true
    },
    "Stock": {
      "type": "integer",
      "default": 0
    },
    "Imagen": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "producto": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::producto.producto",
      "inversedBy": "variantes"
    }
  }
}
